{
  "name": "la-liga",
  "version": "2.0.0",
  "description": "A football manager-type game for e-sports",
  "main": "main.bundle.js",
  "author": "Steve Rivera",
  "license": "UNLICENSED",
  "homepage": "https://github.com/lemonpole/la-liga#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonpole/la-liga.git"
  },
  "bugs": {
    "url": "https://github.com/lemonpole/la-liga/issues"
  },
  "keywords": [
    "manager",
    "e-sports",
    "e-sport",
    "simulator"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production node -r @babel/register ./webpack/compiler.js --mac",
    "db:seed:generate": "node -r @babel/register ./scripts/database-cli.js generate $1",
    "db:seed:all": "node -r @babel/register ./scripts/database-cli.js all $1",
    "start": "cross-env NODE_ENV=development node -r @babel/register ./webpack/compiler.js",
    "start:electron": "cross-env NODE_ENV=development electron ./main.bundle.js",
    "squad-updater": "ts-node ./scripts/squad-updater.ts",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm test -- --coverage"
  },
  "build": {
    "appId": "io.steverivera.laliga",
    "mac": {
      "category": "public.app.category.games",
      "identity": "null"
    },
    "dmg": {
      "title": "${productName}"
    },
    "nsis": {
      "uninstallDisplayName": "${productName}"
    },
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "dist/renderer/windows/**/*",
      "main.bundle.js",
      "resources/**/*"
    ],
    "publish": {
      "provider": "github",
      "releaseType": "release"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@babel/register": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@types/cheerio": "0.22.13",
    "@types/dedent": "0.7.0",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.142",
    "@types/minimist": "1.2.0",
    "@types/mock-fs": "3.6.30",
    "@types/nedb": "1.8.9",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-router-dom": "5.1.0",
    "@types/react-transition-group": "4.2.3",
    "@types/request": "2.48.3",
    "@types/request-promise": "4.1.44",
    "@types/validator": "10.11.3",
    "@types/webpack-env": "1.14.0",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.12.2",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "electron": "6.0.11",
    "electron-builder": "21.2.0",
    "electron-rebuild": "1.8.6",
    "eslint": "6.5.1",
    "eslint-loader": "3.0.2",
    "eslint-plugin-react": "7.16.0",
    "express": "4.17.1",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "mock-fs": "4.10.1",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "typescript": "3.6.3",
    "url-loader": "2.2.0",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0"
  },
  "dependencies": {
    "adjective-animal": "1.1.3",
    "antd": "3.23.6",
    "cheerio": "1.0.0-rc.3",
    "cloudscraper": "4.6.0",
    "console.table": "0.10.0",
    "countries-list": "2.4.3",
    "cuid": "2.1.6",
    "dedent": "0.7.0",
    "duel": "3.2.0",
    "electron-is": "3.0.0",
    "electron-log": "3.0.8",
    "electron-updater": "4.1.2",
    "groupstage": "3.1.1",
    "lodash": "4.17.15",
    "minimist": "1.2.2",
    "nedb": "1.8.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-hot-loader": "4.12.14",
    "react-particles-js": "2.7.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-transition-group": "4.3.0",
    "request": "2.88.0",
    "validator": "11.1.0"
  }
}
